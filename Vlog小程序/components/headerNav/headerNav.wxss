.nav-wrap {
    position: fixed;
    width: 100%;
    top: 0;
    background: #fff;
    color: #000;
    z-index: 9999999;
    display: flex;
    align-items: center;
    border: 1rpx solid #e2e2e2;
}

/* 标题要居中 */

.nav-title {
    position: absolute;
    text-align: center;
    max-width: 400rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    font-size: 36rpx;
    color: #2c2b2b;
    font-weight: 500;
}

.nav-capsule {
    display: flex;
    align-items: center;
    margin-left: 30rpx;
    width: 140rpx;
    justify-content: space-between;
    height: 100%;
}

.navbar-v-line {
    width: 1px;
    height: 32rpx;
    background-color: #e5e5e5;
}

.back-pre, .back-home {
    width: 32rpx;
    height: 36rpx;
    margin-top: 4rpx;
    padding: 10rpx;
}

.nav-capsule .back-home {
    width: 36rpx;
    height: 36rpx;
    margin-top: 8rpx;
}



/* ------------------------------ */
.searchBox{
    width: 500rpx;
    display: flex;
    align-items: center;
    background: #F7F7F7;
    border-radius: 44rpx;
}
.search_icon{
    width: 32rpx;
    height: 33rpx;
    margin: 0 20rpx;
}
.placeActive{
    font-size: 30rpx;
    color: #999999;
}
.navback{
    width: 100%;
    display: flex;
    align-items: center;
}
.title{
    text-align: center;
    max-width: 250rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    font-size: 36rpx;
    color: #2c2b2b;
    font-weight: 500;


}